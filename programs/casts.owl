module casts;

intOverload(x: I32) {
    println("32");
}

intOverload(x: I64) {
    println("64");
}

foo(x: I32) {
    var z = x : I64;
    intOverload(z);
    var y = z : I32;
    intOverload(y);
}

castBool(x: I32) {
    println(x : Bool);
    var b = x > 10;
    println(b : I32);
}

main(argv: String[]) {
    foo(1);
    println("hello" : String);
    castBool(0);
    castBool(12);
}

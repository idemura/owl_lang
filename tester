#! /bin/bash

build_dir="buck-out/gen"
for owl in `find . -name '*.owl'`; do
    out=`tempfile`
    java -jar ${build_dir}/owl_lang_cli.jar $owl > $out
    if [ $? != 0 ]; then
        exit 1
    fi
    diff -Zu $owl.out $out
    if [ $? != 0 ]; then
        exit 1
    fi
done
echo "ALL TESTS PASSED"

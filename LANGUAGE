= Owl Programming Language specification

f(a)[g(b)] += x
f(a) and g(b) are evaluated once

enum Maybe(T) {
    None;
    Just: T;
}

fn test {
    match (open("file")) {
        None: {
            println("error");
        }
        Just content: {
            println(content);
        }
    }
}

const PI = 3.14;
